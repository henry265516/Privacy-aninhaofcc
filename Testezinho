    <script>
        // Criar mais emojis dinamicamente
        function createEmojis() {
            const emojis = ['üçë', 'üî•', 'üíã', 'ü´¶', 'üëÖ', 'üí¶', 'üéØ', 'üçÜ', 'üí¶', 'üòà', 'üëÑ', 'ü•µ'];
            const body = document.body;
            
            for (let i = 0; i < 15; i++) {
                const emoji = document.createElement('div');
                emoji.className = 'emoji';
                emoji.textContent = emojis[Math.floor(Math.random() * emojis.length)];
                
                // Posi√ß√£o aleat√≥ria
                const top = Math.random() * 100;
                const left = Math.random() * 100;
                const delay = Math.random() * 15;
                
                emoji.style.top = `${top}%`;
                emoji.style.left = `${left}%`;
                emoji.style.animationDelay = `${delay}s`;
                emoji.style.fontSize = `${Math.random() * 20 + 20}px`;
                emoji.style.opacity = Math.random() * 0.3 + 0.1;
                
                body.appendChild(emoji);
            }
        }
        
        // Efeitos nos bot√µes
        document.querySelectorAll('.btn').forEach(btn => {
            btn.addEventListener('mouseenter', function() {
                this.style.transform = 'translateY(-5px) scale(1.05)';
            });
            
            btn.addEventListener('mouseleave', function() {
                this.style.transform = 'translateY(0) scale(1)';
            });
            
            // Efeito de clique
            btn.addEventListener('mousedown', function() {
                this.style.transform = 'translateY(-2px) scale(0.98)';
            });
            
            btn.addEventListener('mouseup', function() {
                this.style.transform = 'translateY(-5px) scale(1.05)';
            });
        });
        
        // Efeito pulsante na foto
        const profileImg = document.querySelector('.profile-img');
        function pulseImage() {
            profileImg.style.boxShadow = '0 0 40px rgba(255, 20, 147, 0.8)';
            setTimeout(() => {
                profileImg.style.boxShadow = '0 0 30px rgba(255, 20, 147, 0.6)';
            }, 500);
        }
        
        setInterval(pulseImage, 2000);
        
        // Efeito de digita√ß√£o no t√≠tulo
        const title = document.querySelector('h1');
        const originalText = 'ANA SOUZA';
        title.textContent = '';
        
        let charIndex = 0;
        function typeTitle() {
            if (charIndex < originalText.length) {
                title.textContent += originalText.charAt(charIndex);
                charIndex++;
                setTimeout(typeTitle, 150);
            }
        }
        
        // Iniciar tudo
        setTimeout(() => {
            createEmojis();
            typeTitle();
            pulseImage();
        }, 500);
        
        // Contador de visualiza√ß√µes safado
        const counter = document.createElement('div');
        counter.style.cssText = `
            color: #ff69b4;
            font-size: 13px;
            margin-top: 20px;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 8px;
            opacity: 0.8;
        `;
        counter.innerHTML = `
            <span>üëÅÔ∏è</span>
            ${Math.floor(Math.random() * 1000) + 800} homens j√° me viram
            <span>üëÅÔ∏è</span>
        `;
        document.querySelector('.profile-card').appendChild(counter);
    </script>
